<form method="post" action="{{ path('crud_category_delete', {'id': category.id}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">

    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ category.id) }}">
    <button class="btn w-100">Supprimer</button>
</form>
